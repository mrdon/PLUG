<?xml version='1.0' encoding='ISO-8859-1'?>
<project>
  <pomVersion>3</pomVersion>
  <id>atlassian-plugins-innerjartwo</id>
  <name>Atlassian Plugins Simple Test</name>
  <currentVersion>1.0</currentVersion>
  <organization>
    <name>Atlassian Software Systems Pty Ltd</name>
  </organization>
  <inceptionYear>2007</inceptionYear>
  <package>com.atlassian.plugin.innerjartwo</package>
  <description>Simple test classes for the classloader</description>
  <build>
    <nagEmailAddress>dev@atlassian.com</nagEmailAddress>
    <sourceDirectory>src/java</sourceDirectory>
    <unitTestSourceDirectory>src/test/java</unitTestSourceDirectory>
    <unitTest>
      <includes>
        <include>**/*Test.java</include>
        <include>**/Test*.java</include>
      </includes>
      <excludes>
        <exclude>**/Abstract*.java</exclude>
      </excludes>
      <resources>
        <resource>
          <directory>src/test/etc</directory>
        </resource>
        <resource>
          <directory>src/test/classLoadingTestFiles</directory>
        </resource>
        <resource>
          <directory>src/test/badPluginTestFiles</directory>
        </resource>
      </resources>
    </unitTest>
  </build>
  <dependencies>
  </dependencies>
</project>
